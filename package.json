{
  "name": "tech_news_scrape_back",
  "version": "2.0.0",
  "main": "index.js",
  "license": "ISC",
  "packageManager": "yarn@3.6.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write \"**/*.{ts,json,md}\" \"!package*.json\" \"!tsconfig.json\" \"!.eslintrc.json\"",
    "start": "NODE_ENV=production node --max-old-space-size=700 -r ts-node/register --loader ts-paths-esm-loader --experimental-specifier-resolution=node  ./dist/main.js",
    "build": "NODE_ENV=production tsc --project tsconfig.json --diagnostics  -p tsconfig.json",
    "dev": "NODE_ENV=development node --max-old-space-size=700 -r ts-node/register -r dotenv/config --loader ts-paths-esm-loader --experimental-specifier-resolution=node  ./src/main.ts",
    "schema": "prisma generate",
    "migrate": "prisma migrate dev --name added_melon",
    "db:update": "prisma db push"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@prisma/client": "^5.4.2",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "moment-timezone": "^0.5.43",
    "node-fetch": "^3.3.2",
    "prisma": "^5.4.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "ts-paths-esm-loader": "^1.4.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
